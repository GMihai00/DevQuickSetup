[  
    { "get_reg_val": { 
        "reg_path": "Software\\Microsoft\\Windows\\CurrentVersion\\Run",
        "key_name": "QuickSetupResume",
        "install_key": "RUN_KEY",
        "can_fail": true
        }
    },
    
    { "if": {
        "condition": "%RUN_KEY% != %CMD%",
        "run": [
            { "set_reg_val": {
                "reg_path": "Software\\Microsoft\\Windows\\CurrentVersion\\Run",
                "key_name": "QuickSetupResume",
                "value": "%CMD%"
            }
            },
            { "ps1": { "install_run" : "\"Enable-WindowsOptionalFeature -Online -FeatureName Microsoft-Hyper-V -All\""} }
        ]
    }},

    { "delete_reg_key": { 
        "reg_path": "Software\\Microsoft\\Windows\\CurrentVersion\\Run",
        "key_name": "QuickSetupResume"
        }
    }
    
]